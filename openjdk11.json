{
    "homepage": "http://jdk.java.net/11",
    "version": "11-11",
    "license": "http://openjdk.java.net/legal/gplv2+ce.html",
    "architecture": {
        "64bit": {
            "url": "https://download.java.net/java/early_access/jdk11/11/GPL/openjdk-11-ea+11_windows-x64_bin.tar.gz",
            "hash": "01f205db6ca8c1c4ad404e7ac0a8c808294322e9df2d09d3a8bac2cdf8325bc1"
        }
    },
    "extract_dir": "jdk-11",
    "env_add_path": "bin",
    "env_set": {
        "JAVA_HOME": "$dir"
    },
    "checkver": {
        "re": "openjdk-(?<major>[u\\d]+)-ea\\+(?<build>[b\\d]+)_windows-x64_bin.tar.gz",
        "replace": "${major}-${build}"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://download.java.net/java/early_access/jdk$matchMajor/$matchBuild/GPL/openjdk-$matchMajor-ea+$matchBuild_windows-x64_bin.tar.gz"
            }
        },
        "hash": {
            "url": "$url.sha256"
        }
    }
}
