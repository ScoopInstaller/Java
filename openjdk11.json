{
    "homepage": "http://jdk.java.net/11",
    "version": "11-21",
    "license": "http://openjdk.java.net/legal/gplv2+ce.html",
    "architecture": {
        "64bit": {
            "url": "https://download.java.net/java/early_access/jdk11/21/GPL/openjdk-11-ea+21_windows-x64_bin.zip",
            "hash": "4e20a7042d9d4b7b92d8d8064a7c19905af1b34225907369e632f91d6d876499"
        }
    },
    "extract_dir": "jdk-11",
    "env_add_path": "bin",
    "env_set": {
        "JAVA_HOME": "$dir"
    },
    "checkver": {
        "re": "openjdk-(?<major>[u\\d]+)-ea\\+(?<build>[b\\d]+)_windows-x64_bin.(?<zip>tar.gz|zip)",
        "replace": "${major}-${build}"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://download.java.net/java/early_access/jdk$matchMajor/$matchBuild/GPL/openjdk-$matchMajor-ea+$matchBuild_windows-x64_bin.$matchZip"
            }
        },
        "hash": {
            "url": "$url.sha256"
        }
    }
}
