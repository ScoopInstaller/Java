{
    "homepage": "http://jdk.java.net/11",
    "version": "11-7",
    "license": "http://openjdk.java.net/legal/gplv2+ce.html",
    "architecture": {
        "64bit": {
            "url": "https://download.java.net/java/early_access/jdk11/7/GPL/openjdk-11-ea+7_windows-x64_bin.tar.gz",
            "hash": "2b52461764fbca58f61bc8e813bb7a32e0f1d70a0034dbbcfa553ad8a736083a"
        }
    },
    "extract_dir": "jdk-11",
    "env_add_path": "bin",
    "env_set": {
        "JAVA_HOME": "$dir"
    },
    "checkver": {
        "re": "openjdk-(?<major>[u\\d]+)-ea\\+(?<build>[b\\d]+)_windows-x64_bin.tar.gz",
        "replace": "${major}-${build}"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://download.java.net/java/early_access/jdk$matchMajor/$matchBuild/GPL/openjdk-$matchMajor-ea+$matchBuild_windows-x64_bin.tar.gz"
            }
        },
        "hash": {
            "url": "$url.sha256"
        }
    }
}
