{
    "homepage": "http://jdk.java.net/11",
    "version": "11-10",
    "license": "http://openjdk.java.net/legal/gplv2+ce.html",
    "architecture": {
        "64bit": {
            "url": "https://download.java.net/java/early_access/jdk11/10/GPL/openjdk-11-ea+10_windows-x64_bin.tar.gz",
            "hash": "b1cf902f4829c6d242ae1df6b5efae16db9c95f5f371028752fbf5721fa49079"
        }
    },
    "extract_dir": "jdk-11",
    "env_add_path": "bin",
    "env_set": {
        "JAVA_HOME": "$dir"
    },
    "checkver": {
        "re": "openjdk-(?<major>[u\\d]+)-ea\\+(?<build>[b\\d]+)_windows-x64_bin.tar.gz",
        "replace": "${major}-${build}"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://download.java.net/java/early_access/jdk$matchMajor/$matchBuild/GPL/openjdk-$matchMajor-ea+$matchBuild_windows-x64_bin.tar.gz"
            }
        },
        "hash": {
            "url": "$url.sha256"
        }
    }
}
