{
    "homepage": "http://jdk.java.net/11",
    "version": "11-19",
    "license": "http://openjdk.java.net/legal/gplv2+ce.html",
    "architecture": {
        "64bit": {
            "url": "https://download.java.net/java/early_access/jdk11/19/GPL/openjdk-11-ea+19_windows-x64_bin.zip",
            "hash": "921dc72c317efc1a190b99b852573ce5fc2a04234b5c2b5d1aca9a5c181e7bda"
        }
    },
    "extract_dir": "jdk-11",
    "env_add_path": "bin",
    "env_set": {
        "JAVA_HOME": "$dir"
    },
    "checkver": {
        "re": "openjdk-(?<major>[u\\d]+)-ea\\+(?<build>[b\\d]+)_windows-x64_bin.(?<zip>tar.gz|zip)",
        "replace": "${major}-${build}"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://download.java.net/java/early_access/jdk$matchMajor/$matchBuild/GPL/openjdk-$matchMajor-ea+$matchBuild_windows-x64_bin.$matchZip"
            }
        },
        "hash": {
            "url": "$url.sha256"
        }
    }
}
