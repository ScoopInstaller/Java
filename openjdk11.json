{
    "homepage": "http://jdk.java.net/11",
    "version": "11-9",
    "license": "http://openjdk.java.net/legal/gplv2+ce.html",
    "architecture": {
        "64bit": {
            "url": "https://download.java.net/java/early_access/jdk11/9/GPL/openjdk-11-ea+9_windows-x64_bin.tar.gz",
            "hash": "7377faa5811b65ea01de9bea4ba6e13d0e3622cfbb9a1fecd5975eac29106845"
        }
    },
    "extract_dir": "jdk-11",
    "env_add_path": "bin",
    "env_set": {
        "JAVA_HOME": "$dir"
    },
    "checkver": {
        "re": "openjdk-(?<major>[u\\d]+)-ea\\+(?<build>[b\\d]+)_windows-x64_bin.tar.gz",
        "replace": "${major}-${build}"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://download.java.net/java/early_access/jdk$matchMajor/$matchBuild/GPL/openjdk-$matchMajor-ea+$matchBuild_windows-x64_bin.tar.gz"
            }
        },
        "hash": {
            "url": "$url.sha256"
        }
    }
}
