    "version": "8u172-b03",
    "architecture": {
        "64bit": {
            "url": "https://download.java.net/java/jdk8u172/archive/b03/binaries/jdk-8u172-ea-bin-b03-windows-x64-18_jan_2018.exe#/dl.exe",
            "hash": "d980e72ba906920dcc10f7807c77206cc76c2dea187927ce16b521d6aa458d98"
        },
        "32bit": {
            "url": "https://download.java.net/java/jdk8u172/archive/b03/binaries/jdk-8u172-ea-bin-b03-windows-i586-18_jan_2018.exe#/dl.exe",
            "hash": "e4dd301f3d59ad7a9292632c6bbcf0f7a7d1dfbeeab9284f2fadd2784fdf4194"
        }
    },
    "pre_install": "
Invoke-WebRequest -Uri https://gist.githubusercontent.com/se35710/2c22912a99137263b633e682f3134809/raw/oracle8.ps1 -OutFile \"$dir\\oraclejdk8.ps1\"
    ",
    "installer": {
        "file": "oraclejdk8.ps1",
        "args": [
            "$dir"
        ],
        "keep": "true"
    },
    "env_add_path": "bin",
    "env_set": {
        "JAVA_HOME": "$dir"
    },
    "checkver": {
        "re": "jdk-(?<major>[u\\d]+)-ea-bin-(?<build>[b\\d]+)-windows-(?<arch>[xi4-68]+)-(?<date>[\\w_]+).exe",
        "replace": "${major}-${build}"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://download.java.net/java/jdk$matchMajor/archive/$matchBuild/binaries/jdk-$matchMajor-ea-bin-$matchBuild-windows-x64-$matchDate.exe#/dl.exe",
                "hash": {
                    "url": "$baseurl/jdk-$matchMajor-ea-bin-$matchBuild-windows-x64-$matchDate.sha256",
                    "find": "sha256 \\($basename\\) = ([a-fA-F0-9]{64})"
                }
            },
            "32bit": {
                "url": "https://download.java.net/java/jdk$matchMajor/archive/$matchBuild/binaries/jdk-$matchMajor-ea-bin-$matchBuild-windows-i586-$matchDate.exe#/dl.exe",
                "hash": {
                    "url": "$baseurl/jdk-$matchMajor-ea-bin-$matchBuild-windows-i586-$matchDate.sha256",
                    "find": "sha256 \\($basename\\) = ([a-fA-F0-9]{64})"
                }
            }
        }
    }
}
