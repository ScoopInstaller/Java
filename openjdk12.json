{
    "homepage": "http://jdk.java.net/12",
    "version": "12-3",
    "license": "http://openjdk.java.net/legal/gplv2+ce.html",
    "architecture": {
        "64bit": {
            "url": "https://download.java.net/java/early_access/jdk12/3/GPL/openjdk-12-ea+3_windows-x64_bin.zip",
            "hash": "b90c50461f73999c705fa1b72e68665a837b0a44438b5c337ff7dfdbf356108c"
        }
    },
    "extract_dir": "jdk-12",
    "env_add_path": "bin",
    "env_set": {
        "JAVA_HOME": "$dir"
    },
    "checkver": {
        "re": "openjdk-(?<major>[u\\d]+)-ea\\+(?<build>[b\\d]+)_windows-x64_bin.(?<zip>tar.gz|zip)",
        "replace": "${major}-${build}"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://download.java.net/java/early_access/jdk$matchMajor/$matchBuild/GPL/openjdk-$matchMajor-ea+$matchBuild_windows-x64_bin.$matchZip"
            }
        },
        "hash": {
            "url": "$url.sha256"
        }
    }
}
