{
    "description": "Official production-ready open-source builds of OpenJDK 11",
    "homepage": "https://jdk.java.net/12",
    "version": "12.0.1-12",
    "license": "GPL-2.0-with-classpath-exception",
    "architecture": {
        "64bit": {
            "url": "https://download.java.net/java/GA/jdk12.0.1/69cfe15208a647278a19ef0990eea691/12/GPL/openjdk-12.0.1_windows-x64_bin.zip",
            "hash": "fc7d9eee3c09ea6548b00ca25dbf34a348b3942c815405a1428e0bfef268d08d"
        }
    },
    "extract_dir": "jdk-11.0.2",
    "env_add_path": "bin",
    "env_set": {
        "JAVA_HOME": "$dir"
    },
    "checkver": {
        "re": "/jdk(?<major>[\\d.]+)/(?<random>[a-fA-F0-9]{32})/(?<build>[\\d]+)/GPL/(?<file>openjdk-(?<version>[\\d.]+)(?<ea>-ea)?(?:\\+[\\d]+)?_windows-x64_bin.(zip|tar.gz))",
        "replace": "${version}-${build}${ea}"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://download.java.net/java/GA/jdk$matchMajor/$matchRandom/$matchBuild/GPL/$matchFile"
            }
        },
        "hash": {
            "url": "$url.sha256"
        }
    }
}
