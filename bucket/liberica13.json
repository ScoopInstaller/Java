{
    "description": "BellSoft Liberica is a 100% open-source Java 13 implementation",
    "homepage": "https://bell-sw.com/pages/java-13",
    "version": "13.0.2",
    "license": "GPL-2.0-only WITH Classpath-exception-2.0",
    "architecture": {
        "64bit": {
            "url": "https://download.bell-sw.com/java/13.0.2/bellsoft-jdk13.0.2-windows-amd64.zip",
            "hash": "sha1:653876CD70A20ED8EAA3A6FE915C5FA02FDF2544"
        },
        "32bit": {
            "url": "https://download.bell-sw.com/java/13.0.2/bellsoft-jdk13.0.2-windows-i586.zip",
            "hash": "sha1:3152D20417C78522595F63E2CD6A22C2677E367C"
        }
    },
    "extract_dir": "jdk-13.0.2",
    "env_add_path": "bin",
    "env_set": {
        "JAVA_HOME": "$dir"
    },
    "checkver": {
        "re": "bs-link-current-release(?:.*?)/pages/java-(13[\\d.]+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://download.bell-sw.com/java/$version/bellsoft-jdk$version-windows-amd64.zip"
            },
            "32bit": {
                "url": "https://download.bell-sw.com/java/$version/bellsoft-jdk$version-windows-i586.zip"
            }
        },
        "hash": {
            "url": "https://bell-sw.com/pages/java-$version/",
            "find": "(?smi)$url(?:.*?)<code>($sha1)</code>"
        },
        "extract_dir": "jdk-$version"
    }
}
