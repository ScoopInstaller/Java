{
    "description": "AdoptOpenJDK 11 JRE with Eclipse OpenJ9 JVM",
    "homepage": "https://adoptopenjdk.net",
    "version": "11.0.5-10.1-0.17.0",
    "license": "GPL-2.0-only WITH Classpath-exception-2.0,EPL-2.0",
    "architecture": {
        "64bit": {
            "url": "https://github.com/AdoptOpenJDK/openjdk11-binaries/releases/download/jdk-11.0.5%2B10.1_openj9-0.17.0/OpenJDK11U-jre_x64_windows_openj9_11.0.5_10_openj9-0.17.0.zip",
            "hash": "57ed3426c0a0ecdd9fc18d314a63cc116ae68a9cb355460daaa94091000ccda3"
        }
    },
    "extract_dir": "jdk-11.0.5+10-jre",
    "env_add_path": "bin",
    "env_set": {
        "JAVA_HOME": "$dir"
    },
    "checkver": {
        "url": "https://api.adoptopenjdk.net/v2/info/releases/openjdk11?openjdk_impl=openj9&os=windows&arch=x64&release=latest&type=jre",
        "jp": "$.binaries[0].binary_link",
        "re": "https://github.com/(?<url>.*?(?<tag>jdk-(?<major>(?<jdk>[\\d]+)[\\d.]+)(?:\\%2B|\\+)(?<build>(?<bmajor>[\\d]+)[\\d.]*)(?:(?<oj>_openj9?)(?<jvmver>-[\\d.]+))?)/.*?).zip",
        "replace": "${major}-${build}${jvmver}"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/$matchUrl.zip"
            }
        },
        "hash": {
            "url": "$url.sha256.txt",
            "find": "^([a-fA-F0-9]+)\\s"
        },
        "extract_dir": "jdk-$matchMajor+$matchBmajor-jre"
    }
}
