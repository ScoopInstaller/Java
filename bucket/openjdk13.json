{
    "description": "Official production-ready open-source builds of OpenJDK 13",
    "homepage": "http://jdk.java.net/13",
    "version": "13-11-ea",
    "license": "GPL-2.0-with-classpath-exception",
    "architecture": {
        "64bit": {
            "url": "https://download.java.net/java/early_access/jdk13/11/GPL/openjdk-13-ea+11_windows-x64_bin.zip",
            "hash": "af7bf81dc9c78bc4c58c8aef2001d31c34417a35d99087e97464fd59323a009e"
        }
    },
    "extract_dir": "jdk-13",
    "env_add_path": "bin",
    "env_set": {
        "JAVA_HOME": "$dir"
    },
    "checkver": {
        "re": "/(?<type>early_access|GA)/jdk(?<major>[\\d]+)/(?<build>[\\d]+)/GPL/openjdk-(?:[\\d]+)(?<ea>(-ea)?)(?<eabuild>[+\\d]*)_windows-x64_bin.(?<zip>tar.gz|zip)",
        "replace": "${major}-${build}${ea}"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://download.java.net/java/$matchType/jdk$matchMajor/$matchBuild/GPL/openjdk-$matchMajor$matchEa$matchEabuild_windows-x64_bin.$matchZip"
            }
        },
        "hash": {
            "url": "$url.sha256"
        }
    }
}
