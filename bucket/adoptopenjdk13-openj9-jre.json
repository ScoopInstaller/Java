{
    "description": "AdoptOpenJDK 13 JRE with Eclipse OpenJ9 JVM",
    "homepage": "https://adoptopenjdk.net",
    "version": "13.0.1-9.1-0.17.0",
    "license": "GPL-2.0-only WITH Classpath-exception-2.0,EPL-2.0",
    "architecture": {
        "64bit": {
            "url": "https://github.com/AdoptOpenJDK/openjdk13-binaries/releases/download/jdk-13.0.1%2B9.1_openj9-0.17.0/OpenJDK13U-jre_x64_windows_openj9_13.0.1_9_openj9-0.17.0.zip",
            "hash": "3a6369a72484673f46f37c11cd21ffe8b7e338b0bd652eb56eb39e8268c26905"
        }
    },
    "extract_dir": "jdk-13.0.1+9-jre",
    "env_add_path": "bin",
    "env_set": {
        "JAVA_HOME": "$dir"
    },
    "checkver": {
        "url": "https://api.adoptopenjdk.net/v2/info/releases/openjdk13?openjdk_impl=openj9&os=windows&arch=x64&release=latest&type=jre",
        "jp": "$.binaries[0].binary_link",
        "re": "https://github.com/(?<url>.*?(?<tag>jdk-(?<major>(?<jdk>[\\d]+)[\\d.]+)(?:\\%2B|\\+)(?<build>(?<bmajor>[\\d]+)[\\d.]*)(?:(?<oj>_openj9?)(?<jvmver>-[\\d.]+))?)/.*?).zip",
        "replace": "${major}-${build}${jvmver}"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/$matchUrl.zip"
            }
        },
        "hash": {
            "url": "$url.sha256.txt",
            "find": "^([a-fA-F0-9]+)\\s"
        },
        "extract_dir": "jdk-$matchMajor+$matchBmajor-jre"
    }
}
