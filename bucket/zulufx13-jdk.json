    "architecture": {
        "64bit": {
            "url": "https://cdn.azul.com/zulu/bin/zulu13.42.17-ca-fx-jdk13.0.8-win_x64.zip",
            "hash": "7bce8c47f0db0179a6cdd375e468bdfbc4a7b7d3801e27cee25aaeaafc728ac1",
            "extract_dir": "zulu13.42.17-ca-fx-jdk13.0.8-win_x64"
        },
        "32bit": {
            "url": "https://cdn.azul.com/zulu/bin/zulu13.42.17-ca-fx-jdk13.0.8-win_i686.zip",
            "hash": "bb00b378dd9707bd23c6c8f076d7d47e65473bbbe72848d5aa61c62896c92414",
            "extract_dir": "zulu13.42.17-ca-fx-jdk13.0.8-win_i686"
        }
    },
    "env_add_path": "bin",
    "env_set": {
        "JAVA_HOME": "$dir"
    },
    "checkver": {
        "url": "https://api.azul.com/zulu/download/community/v1.0/bundles/latest/?jdk_version=13&bundle_type=jdk&features=fx&ext=zip&os=windows&arch=x86&hw_bitness=64",
        "jsonpath": "$.name",
        "regex": "(?<name>zulu(?<version>[\\d.]+)-ca-fx-jdk(?<java>[\\d.]+)-win)_x64.zip",
        "replace": "${version}"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://cdn.azul.com/zulu/bin/$matchName_x64.zip",
                "extract_dir": "$matchName_x64",
                "hash": {
                    "url": "https://api.azul.com/zulu/download/community/v1.0/bundles/latest/?jdk_version=13&bundle_type=jdk&features=fx&ext=zip&os=windows&arch=x86&hw_bitness=64",
                    "jp": "$.sha256_hash"
                }
            },
            "32bit": {
                "url": "https://cdn.azul.com/zulu/bin/$matchName_i686.zip",
                "extract_dir": "$matchName_i686",
                "hash": {
                    "url": "https://api.azul.com/zulu/download/community/v1.0/bundles/latest/?jdk_version=13&bundle_type=jdk&features=fx&ext=zip&os=windows&arch=x86&hw_bitness=32",
                    "jp": "$.sha256_hash"
                }
            }
        }
    }
}
