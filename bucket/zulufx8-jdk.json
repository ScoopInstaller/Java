    },
    "architecture": {
        "64bit": {
            "url": "https://cdn.azul.com/zulu/bin/zulu8.64.0.19-ca-fx-jdk8.0.345-win_x64.zip",
            "hash": "58c85189e53f86b0283e419b5523e885feed77a2ba3e4f40203111eade68ea0d",
            "extract_dir": "zulu8.64.0.19-ca-fx-jdk8.0.345-win_x64"
        },
        "32bit": {
            "url": "https://cdn.azul.com/zulu/bin/zulu8.64.0.19-ca-fx-jdk8.0.345-win_i686.zip",
            "hash": "04ff4941ccb3707296fb4b7bb374b181a7bf0ac3d6413915ef67baeae975ffaa",
            "extract_dir": "zulu8.64.0.19-ca-fx-jdk8.0.345-win_i686"
        }
    },
    "env_add_path": "bin",
    "env_set": {
        "JAVA_HOME": "$dir"
    },
    "checkver": {
        "url": "https://api.azul.com/zulu/download/community/v1.0/bundles/latest/?jdk_version=8&bundle_type=jdk&features=fx&javafx=true&ext=zip&os=windows&arch=x86&hw_bitness=64",
        "jsonpath": "$.name",
        "regex": "(?<name>zulu(?<version>[\\d.]+)-ca-fx-jdk(?<java>[\\d.]+)-win)_x64.zip",
        "replace": "${version}"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://cdn.azul.com/zulu/bin/$matchName_x64.zip",
                "extract_dir": "$matchName_x64",
                "hash": {
                    "url": "https://api.azul.com/zulu/download/community/v1.0/bundles/latest/?jdk_version=8&bundle_type=jdk&features=fx&javafx=true&ext=zip&os=windows&arch=x86&hw_bitness=64",
                    "jp": "$.sha256_hash"
                }
            },
            "32bit": {
                "url": "https://cdn.azul.com/zulu/bin/$matchName_i686.zip",
                "extract_dir": "$matchName_i686",
                "hash": {
                    "url": "https://api.azul.com/zulu/download/community/v1.0/bundles/latest/?jdk_version=8&bundle_type=jdk&features=fx&javafx=true&ext=zip&os=windows&arch=x86&hw_bitness=32",
                    "jp": "$.sha256_hash"
                }
            }
        }
    }
}
