            "hash": "39303de7c6a330af469906b58fc4ccc0068ce36b05d631d1a0ebf780321d8064"
        },
        "32bit": {
            "url": "https://download2.gluonhq.com/openjfx/18/openjfx-18-ea%2B11_windows-x86_bin-jmods.zip",
            "hash": "6c12b2aa085e77e8785d25ebc90877c4554ba456cd0ae3612d51d1454976c34b"
