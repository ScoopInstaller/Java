{
    "description": "AdoptOpenJDK 11 with Eclipse OpenJ9 JVM with large heap",
    "homepage": "https://adoptopenjdk.net",
    "version": "11.0.6-10-0.18.1",
    "license": "GPL-2.0-only WITH Classpath-exception-2.0,EPL-2.0",
    "architecture": {
        "64bit": {
            "url": "https://github.com/AdoptOpenJDK/openjdk11-binaries/releases/download/jdk-11.0.6%2B10_openj9-0.18.1/OpenJDK11U-jdk_x64_windows_openj9_windowsXL_11.0.6_10_openj9-0.18.1.zip",
            "hash": "82cd404ca2d847a64688f37960e85c2f16cbe06e7a6f43a4b95163152f6ea343"
        }
    },
    "extract_dir": "jdk-11.0.6+10",
    "env_add_path": "bin",
    "env_set": {
        "JAVA_HOME": "$dir"
    },
    "checkver": {
        "url": "https://api.adoptopenjdk.net/v2/info/releases/openjdk11?openjdk_impl=openj9&heap_size=large&os=windows&arch=x64&release=latest&type=jdk",
        "jp": "$.binaries[0].binary_link",
        "re": "https://github.com/(?<url>.*?(?<tag>jdk-(?<major>(?<jdk>[\\d]+)[\\d.]+)(?:\\%2B|\\+)(?<build>[\\d]+)(?<patch>[\\d.]*)(?:(?<oj>_openj9?)(?<jvmver>-[\\d.]+))?)/.*?).zip",
        "replace": "${major}-${build}${patch}${jvmver}"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/$matchUrl.zip"
            }
        },
        "hash": {
            "url": "$url.sha256.txt",
            "find": "^([a-fA-F0-9]+)\\s"
        },
        "extract_dir": "jdk-$matchMajor+$matchBuild"
    }
}
