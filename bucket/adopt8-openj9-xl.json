    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/$matchUrl.zip"
            }
        },
        "hash": {
            "url": "$url.sha256.txt",
            "find": "^([a-fA-F0-9]+)\\s"
