    "architecture": {
        "64bit": {
            "url": "https://cdn.azul.com/zulu/bin/zulu13.50.15-ca-fx-jre13.0.12-win_x64.zip",
            "hash": "94db844f219935385a7d82f2a799df8a1aab14ce8b9667bc3a16c63cd3637fdc",
            "extract_dir": "zulu13.50.15-ca-fx-jre13.0.12-win_x64"
        },
        "32bit": {
            "url": "https://cdn.azul.com/zulu/bin/zulu13.50.15-ca-fx-jre13.0.12-win_i686.zip",
            "hash": "609b9463633bdffaef46e0bc5e139e76765a8b5def3f751a5c01b5ae12093bc2",
            "extract_dir": "zulu13.50.15-ca-fx-jre13.0.12-win_i686"
        }
    },
    "env_add_path": "bin",
    "env_set": {
        "JAVA_HOME": "$dir"
    },
    "checkver": {
        "url": "https://api.azul.com/zulu/download/community/v1.0/bundles/latest/?jdk_version=13&bundle_type=jre&features=headfull,fx&javafx=true&ext=zip&os=windows&arch=x86&hw_bitness=64",
        "jsonpath": "$.name",
        "regex": "(?<name>zulu(?<version>[\\d.]+)-ca-fx-jre(?<java>[\\d.]+)-win)_x64.zip",
        "replace": "${version}"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://cdn.azul.com/zulu/bin/$matchName_x64.zip",
                "extract_dir": "$matchName_x64",
                "hash": {
                    "url": "https://api.azul.com/zulu/download/community/v1.0/bundles/latest/?jdk_version=13&bundle_type=jre&features=headfull,fx&javafx=true&ext=zip&os=windows&arch=x86&hw_bitness=64",
                    "jp": "$.sha256_hash"
                }
            },
            "32bit": {
                "url": "https://cdn.azul.com/zulu/bin/$matchName_i686.zip",
                "extract_dir": "$matchName_i686",
                "hash": {
                    "url": "https://api.azul.com/zulu/download/community/v1.0/bundles/latest/?jdk_version=13&bundle_type=jre&features=headfull,fx&javafx=true&ext=zip&os=windows&arch=x86&hw_bitness=32",
                    "jp": "$.sha256_hash"
                }
            }
        }
    }
}
