                "hash": {
                    "url": "https://github.com/corretto/corretto-jdk/releases/tag/$version",
                    "regex": "(?smi)amazon-corretto-$version-windows-x64-jdk.zip(?:.*?)<code>([a-fA-F0-9]+)</code>"
                }
            }
        },
        "extract_dir": "jdk$matchHead_$buildVersion"
    }
}
