    "license": {
        "identifier": "GPL-2.0-only WITH Classpath-exception-2.0",
        "url": "https://www.azulsystems.com/license/zulu_third_party_licenses.html"
    },
    "architecture": {
        "64bit": {
            "url": "https://cdn.azul.com/zulu/bin/zulu17.28.13-ca-fx-jdk17.0.0-win_x64.zip",
            "hash": "e4d1a4bc7c30789c33c85ab346cbe53a7555e2ccd3efadaebaffb3c14a037505",
            "extract_dir": "zulu17.28.13-ca-fx-jdk17.0.0-win_x64"
        },
        "32bit": {
            "url": "https://cdn.azul.com/zulu/bin/zulu17.28.13-ca-fx-jdk17.0.0-win_i686.zip",
            "hash": "bb00b378dd9707bd23c6c8f076d7d47e65473bbbe72848d5aa61c62896c92414",
            "extract_dir": "zulu17.28.13-ca-fx-jdk17.0.0-win_i686"
        }
    },
    "env_add_path": "bin",
    "env_set": {
        "JAVA_HOME": "$dir"
    },
    "checkver": {
        "url": "https://api.azul.com/zulu/download/community/v1.0/bundles/latest/?jdk_version=&bundle_type=jdk&features=fx&ext=zip&os=windows&arch=x86&hw_bitness=64",
        "jsonpath": "$.name",
        "regex": "(?<name>zulu(?<version>[\\d.]+)-ca-fx-jdk(?<java>[\\d.]+)-win)_x64.zip",
        "replace": "${version}"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://cdn.azul.com/zulu/bin/$matchName_x64.zip",
                "extract_dir": "$matchName_x64",
                "hash": {
                    "url": "https://api.azul.com/zulu/download/community/v1.0/bundles/latest/?jdk_version=&bundle_type=jdk&features=fx&ext=zip&os=windows&arch=x86&hw_bitness=64",
                    "jp": "$.sha256_hash"
                }
            },
            "32bit": {
                "url": "https://cdn.azul.com/zulu/bin/$matchName_i686.zip",
                "extract_dir": "$matchName_i686",
                "hash": {
                    "url": "https://api.azul.com/zulu/download/community/v1.0/bundles/latest/?jdk_version=&bundle_type=jdk&features=fx&ext=zip&os=windows&arch=x86&hw_bitness=32",
                    "jp": "$.sha256_hash"
                }
            }
        }
    }
}
