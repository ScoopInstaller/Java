            "hash": "fbc190923f4a5f084cdb2f217adcbf1d14572eb015e2041bce928d2ecd71cd8a"
        },
        "32bit": {
            "url": "https://download.java.net/java/jdk8u172/archive/b03/binaries/jre-8u172-ea-bin-b03-windows-i586-18_jan_2018.exe#/dl.exe",
            "hash": "3309bea599a6781a941db8388b31acaf34ae4074c7a46956b0fbfe6242f773b4"
        }
    },
    "pre_install": "
Invoke-WebRequest -Uri https://gist.githubusercontent.com/se35710/2c22912a99137263b633e682f3134809/raw/oracle8.ps1 -OutFile \"$dir\\oraclejdk8.ps1\"
    ",
    "installer": {
        "file": "oraclejdk8.ps1",
        "args": [
            "$dir"
        ],
        "keep": "true"
    },
    "env_add_path": "bin",
    "env_set": {
        "JAVA_HOME": "$dir"
    },
    "checkver": {
        "re": "jre-(?<major>[u\\d]+)-ea-bin-(?<build>[b\\d]+)-windows-(?<arch>[xi4-68]+)-(?<date>[\\w_]+).exe",
        "replace": "${major}-${build}"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://download.java.net/java/jdk$matchMajor/archive/$matchBuild/binaries/jre-$matchMajor-ea-bin-$matchBuild-windows-x64-$matchDate.exe#/dl.exe",
                "hash": {
                    "url": "$baseurl/jre-$matchMajor-ea-bin-$matchBuild-windows-x64-$matchDate.sha256",
                    "find": "sha256 \\($basename\\) = ([a-fA-F0-9]{64})"
                }
            },
            "32bit": {
                "url": "https://download.java.net/java/jdk$matchMajor/archive/$matchBuild/binaries/jre-$matchMajor-ea-bin-$matchBuild-windows-i586-$matchDate.exe#/dl.exe",
                "hash": {
                    "url": "$baseurl/jre-$matchMajor-ea-bin-$matchBuild-windows-i586-$matchDate.sha256",
                    "find": "sha256 \\($basename\\) = ([a-fA-F0-9]{64})"
                }
            }
        }
    }
}
